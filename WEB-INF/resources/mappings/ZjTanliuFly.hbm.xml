<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping  PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.tried.zjsys.testDataSrc.model">
  <class name="ZjTanliuFly" table="tried_zj_tanliu_fly">
    <comment>-</comment>
    <id name="id">
      <generator class="uuid.hex"/>
    </id>
    <property name="srcId">
      <column name="srcId"/>
    </property>
    <property name="circleId">
      <column name="circleId"/>
    </property>
    <property name="deviceNum">
      <column name="deviceNum"/>
    </property>
    <property name="sampleNum">
      <column name="sampleNum"/>
    </property>
    <property name="time">
      <column name="time"/>
    </property>
    <property name="resultc">
      <column name="resultc"/>
    </property>
    <property name="results">
      <column name="results"/>
    </property>
    <property name="recordTime">
      <column name="recordTime"/>
    </property>
    <property name="recordUser">
      <column name="recordUser"/>
    </property>
    <property name="dataStatus">
      <column name="dataStatus"/>
    </property>
      <property name="recordUserName">
      	   <formula>
      	(select tsu.userName from tried_system_user tsu where tsu.id=recordUser)
      </formula>
    </property>
    
  </class>
</hibernate-mapping>
