<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping  PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.tried.system.model">
  <class name="SystemSerialMax" table="tried_system_serial_max">
    <comment>-</comment>
    <id name="id">
      <generator class="uuid.hex"/>
    </id>
    <property name="serialId">
      <column name="serialId"/>
    </property>
    <property name="sysStandardValue">
      <column name="sysStandardValue"/>
    </property>
    <property name="sysSerialNum">
      <column name="sysSerialNum"/>
    </property>
    <property name="recordTime">
      <column name="recordTime"/>
    </property>
    <property name="recordUser">
      <column name="recordUser"/>
    </property>
  </class>
</hibernate-mapping>
