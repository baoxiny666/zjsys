<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping  PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.tried.system.model">
  <class name="SystemLoginInfo" table="tried_system_login_info">
    <comment>用户登录记录表</comment>
    <id name="id">
      <generator class="uuid.hex"/>
    </id>
    <property name="recordUser">
      <column name="recordUser"/>
    </property>
    <property name="loginName">
     	<column name="loginName"/>
    </property>
    <property name="recordTime">
      <column name="recordTime"/>
    </property>
    <property name="loginIP">
      <column name="loginIP"/>
    </property>
    <property name="loginAddress">
      <column name="loginAddress"/>
    </property>
    <property name="context">
      <column name="context"/>
    </property>
  </class>
</hibernate-mapping>
